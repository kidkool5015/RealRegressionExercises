<Window x:Class="RealRegressionExercises.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RealRegressionExercises"
        xmlns:scottplot="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="810">
    <Grid Margin="0,0,400,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition Width="37*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <scottplot:WpfPlot x:Name="MyWpfPlot" Margin="10,0,10,78" Grid.ColumnSpan="2" />
        <Grid Margin="10,0,0,0" HorizontalAlignment="Left" Width="810" RenderTransformOrigin="0.178,0.458" Height="382" VerticalAlignment="Center" Grid.ColumnSpan="4">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="83*"/>
                <ColumnDefinition Width="79*"/>
            </Grid.ColumnDefinitions>
        </Grid>
        <Button Margin="70,26,-320,343" Grid.Column="2" Click="Button_Click" Grid.ColumnSpan="2">
            <TextBlock TextWrapping="Wrap" Text="Generate Graph"/>
        </Button>
        <TextBox Margin="148,250,-242,138" Grid.Column="2" Grid.ColumnSpan="2" TextChanged="TextBox_TextChanged"/>
        <TextBlock Margin="168,222,-222,189" Grid.Column="2" Grid.ColumnSpan="2" ><Run Language="en-ca" Text="X-Axis"/></TextBlock>
        <TextBlock x:Name="ListBox" Margin="45,100,-345,217" Grid.Column="2" Grid.ColumnSpan="2" ><Run Language="en-ca" Text="Which data do you want to see from the chosen Data Set?"/><LineBreak/><Run Language="en-ca" Text="1-"/><LineBreak/><Run Language="en-ca" Text="2-"/><LineBreak/><Run Language="en-ca" Text="3-"/><LineBreak/><Run Language="en-ca" Text="4-"/><LineBreak/><Run Language="en-ca" Text="5-"/></TextBlock>
        <TextBlock Margin="172,301,-226,110" Grid.Column="2" Grid.ColumnSpan="2" ><Run Language="en-ca" Text="Y"/><Run Text="-Axis"/></TextBlock>
        <TextBox Margin="148,329,-242,57" Grid.Column="2" Grid.ColumnSpan="2" TextChanged="TextBox_TextChanged_1" />
        <TextBlock x:Name="BottomBox" Margin="65,331,40,26" Grid.Column="1" ><Run Language="en-ca" Text="XDXDXD"/></TextBlock>
    </Grid>
</Window>